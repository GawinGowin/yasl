#!/usr/local/bin/yasl
#!

{1:=:1 1:print "\n" 0:print} _print_stack1
{2:=:2 2:print "\n" 0:print} _print_stack2

{0:=:1 0:!:0} push_stack1
{0:=:2 0:!:0} push_stack2
{[#]} strlen

{
  1 push_stack2            // ret
  0:=:1 !
  1:strlen:1               // n = strlen(str); n = i;
  1:%@ (                   // while (i > 0)
    1 push_stack1 -:1      // i = i - 1;
    %]#[:1 =:1             // get str[i], dup str[i]

    "0" push_stack1
    2 push_stack1 ^:1 >:2  // "0" > "str[i]" = tmp1
    "9" push_stack1
    2 push_stack1 ^:1 <:2  // "9" < "str[i]" = tmp1
    2:||:1 ? (             // tmp1 || tmp1
      1:(=:1 -:1)
      0 push_stack2 2:&:2
    )
  )
  1:(!!)
  2:=:0 2:!
} isdigit // Use stack:1, 2

{
=
"str: `" print
print "`\nisdigit: " print
isdigit print "\n" print 
} _test

"!#%"
_test
"01234"
_test 
"9412324"
_test 
"12A"
_test 
"z1324"
_test

// 0:#:0
// print "\n" print
// 1:#:0
// print "\n" print
// 2:#:0
// print "\n" print